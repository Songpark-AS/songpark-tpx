 :logger {:sentry-settings {:log? false
                            :dsn ""}}
 
 :network {:config-dir "/etc/network/interfaces.d/"
           :check-network-status-cmd "ip -o l show eth1 | cut -d ' ' -f 9 | tr -d '\n'"
